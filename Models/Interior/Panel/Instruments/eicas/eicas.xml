<PropertyList>
  
  <path>eicas.ac</path>
  
  <animation>
    <object-name>Bezel</object-name>
    <object-name>btn</object-name>
    <object-name>EICAS.menu</object-name>
    <object-name>EICAS.norm</object-name>
    <object-name>LH-N1</object-name>
    <object-name>RH-N1</object-name>
    <object-name>LH-ITT</object-name>
    <object-name>RH-ITT</object-name>
    <object-name>oiltemp1</object-name>
    <object-name>oiltemp2</object-name>
    <object-name>oilpsi1</object-name>
    <object-name>oilpsi2</object-name>
    <object-name>STABneedle</object-name>
    <object-name>Flapneedle</object-name>
    <object-name>slats.annun</object-name>
    <object-name>spoilers.annun</object-name>
    <object-name>gear1.annun</object-name>
    <object-name>gear0.annun</object-name>
    <object-name>gear2.annun</object-name>
  </animation>
  
  <animation>
    <type>material</type>
    <object-name>btn</object-name>
    <object-name>EICAS.menu</object-name>
    <object-name>EICAS.norm</object-name>
    <object-name>LH-N1</object-name>
    <object-name>RH-N1</object-name>
    <object-name>LH-ITT</object-name>
    <object-name>RH-ITT</object-name>
    <object-name>oiltemp1</object-name>
    <object-name>oiltemp2</object-name>
    <object-name>oilpsi1</object-name>
    <object-name>oilpsi2</object-name>
    <object-name>STABneedle</object-name>
    <object-name>Flapneedle</object-name>
    <object-name>slats.annun</object-name>
    <object-name>spoilers.annun</object-name>
    <object-name>gear1.annun</object-name>
    <object-name>gear0.annun</object-name>
    <object-name>gear2.annun</object-name>
    <condition>
      <property>controls/lighting/instrument-lights</property>
      <property>/controls/electric/battery-switch</property>
    </condition>
    <emission>
      <factor-prop>controls/lighting/luminosite-lights</factor-prop>
      <red>   1 </red>
      <green> 1 </green>
      <blue>  1 </blue>
    </emission>
  </animation>
  
  <panel>
    <path>Aircraft/Embraer-ERJ-145/Models/Interior/Panel/Instruments/eicas/EICAS-panel.xml</path>
    <bottom-left>
      <x-m>  0.003 </x-m>
      <y-m> -0.076 </y-m>
      <z-m> -0.077 </z-m>
    </bottom-left>
    <bottom-right>
      <x-m>  0.003 </x-m>
      <y-m>  0.076 </y-m>
      <z-m> -0.077 </z-m>
    </bottom-right>
    <top-left>
      <x-m>  0.003 </x-m>
      <y-m> -0.076 </y-m>
      <z-m>  0.106 </z-m>
    </top-left>
  </panel>
