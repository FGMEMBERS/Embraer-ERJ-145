<?xml version="1.0" encoding="ISO-8859-1"?>

<PropertyList>

  <sim include="Systems/views.xml">

    <description>Embraer ERJ 145 (YASim)</description>
    <author>Emmanuel BARANGER (3D/FDM)</author>
    <aircraft-version>0.1</aircraft-version>
    <status>Alpha</status>

    <fuel-fraction>1.0</fuel-fraction>

    <startup>
      <splash-texture>Aircraft/Embraer-ERJ-145/erj145-splash.png</splash-texture>
    </startup>

    <sound>
      <path archive="y">Aircraft/Embraer-ERJ-145/Sounds/erj145-sound.xml</path>
    </sound>

    <model>
      <path archive="y">Aircraft/Embraer-ERJ-145/Models/erj145.xml</path>
      <livery>
        <file type="string">default</file>
      </livery>
    </model>

    <instrumentation>
      <path>Aircraft/Embraer-ERJ-145/Systems/instrumentation.xml</path>
    </instrumentation>
    
    <systems>
      <path>Aircraft/Embraer-ERJ-145/Systems/systems.xml</path>
      <!-- <autopilot>
        <path>Aircraft/Embraer-ERJ-145/Systems/autopilot.xml</path>
      </autopilot> -->
    </systems>
    
    <multiplay>
      <chat_display>1</chat_display>
      <generic> 
        <int n="0" type="int">  0 </int>                                              <!--  0 immat -->
        <int n="1" type="int">  0 </int>                                              <!--  1 immat -->
        <int n="2" type="int">  0 </int>                                              <!--  2 immat -->
        <int n="3" type="int">  0 </int>                                              <!--  3 immat -->
        <int n="4" type="int">  0 </int>                                              <!--  4 immat -->
        <int n="5" type="int">  0 </int>                                              <!--  5 immat -->
        <int n="6" type="bool"> 1 </int>                                              <!-- 6 smoke/bomb/torpedo -->
        <int n="7" type="int" alias="/sim/model/merchantman">0</int>                  <!-- 7 Mode 0=passenger 1=Mixte 2=Cargo -->
        <int n="8" type="int" alias="/systems/electrical/outputs/nav-lights">0</int>  <!-- 8 nav-lights -->
        <int n="9" type="int" alias="/sim/model/lights/beacon/state">false</int>      <!-- 9 strobe-lights -->
        <int n="10" type="int" alias="/controls/electric/key-pos">0</int>             <!-- 10 key position -->
        <float n="0" alias="/sim/model/door-positions/crew/position-norm"/>
        <float n="1" alias="/sim/model/door-positions/passenger/position-norm"/>
        <float n="2" alias="/position/gear-agl-m"> 0 </float>                         <!-- altitude for MP shadow -->
        <float n="3" alias="/position/altitude-ft"> 0 </float>                        <!-- altitude for Smoke/Trail -->
        <float n="4" alias="/engines/engine[0]/n1"/>
        <float n="5" alias="/engines/engine[1]/n1"/>
        <float n="6" alias="/engines/engine[0]/n2"/>
        <float n="7" alias="/engines/engine[1]/n2"/>
        <float n="8" alias="/gear/gear[0]/wow/"/>
        <float n="9" alias="/gear/gear[1]/wow/"/>
        <float n="10" alias="/gear/gear[2]/wow/"/>
        <float n="11" alias="/gear/gear[0]/rollspeed-ms"/>
        <float n="12" alias="/gear/gear[1]/rollspeed-ms"/>
        <float n="13" alias="/gear/gear[2]/rollspeed-ms"/>
        <!-- For Bombable -->
        <int type="int" n="10" alias="controls/armament/trigger">0</int>
        <string n="9" type="string" />
      </generic>
    </multiplay>
    
    <help include="Systems/help.xml"/>
    
    <menubar include="Dialogs/erj145-menu.xml"/>

  </sim>
  
  <instrumentation>
    <mk-viii>                                            <!-- http://wiki.flightgear.org/index.php/GPWS -->
      <serviceable>true</serviceable>                    <!-- EGPWS_ENABLE -->
      <configuration-module>
        <category-1>   1 </category-1>                   <!-- AIRCRAFT_MODE_TYPE_SELECT -->
        <category-8>                                     <!-- RADIO_ALTITUDE_INPUT_SELECT -->
          3                                              <!-- YASim --> 
        </category-8>
      </configuration-module>
      <inputs>                                           <!-- Module I/O -->
        <arinc429>
          <decision-height-ncd> false </decision-height-ncd>
          <decision-height> 250 </decision-height>
        </arinc429>
      </inputs>
      <speaker>
        <volume>0.4</volume>
      </speaker>
    </mk-viii>
    <tcas>
      <serviceable type="bool">true</serviceable>
      <inputs>
        <mode type="int">0</mode>                        <!-- 0=off, 1=standby, 2=TA-only, 3=auto(TA/RA) -->
      </inputs>
      <!--voice>
      <file-prefix type="string">Sounds/tcas/</file-prefix>
      </voice-->
      <speaker>                                          <!-- Speaker Configuration -->
        <max-dist type="double">7</max-dist>             <!-- Max. distance where speaker is heard -->
        <reference-dist type="double">5</reference-dist> <!-- Distance to pilot -->
        <volume type="double"> 1 </volume>               <!-- Volume at reference distance -->
      </speaker>
      <range type="int">6</range>
    </tcas>
    <nd>
      <radar-texture-path type="string">Aircraft/Embraer-ERJ-145/Models/Interior/Panel/Instruments/mfd/radar.png</radar-texture-path>
      <symbol-texture-path type="string">Aircraft/Embraer-ERJ-145/Models/Interior/Panel/Instruments/mfd/symbols.png</symbol-texture-path>
      <font>
        <color>
          <red type="float">0.7</red>
          <green type="float">0.7</green>
          <blue type="float">0.7</blue>
          <alpha type="float">1</alpha>
        </color>
        <name type="string">helvetica_bold.txf</name>
        <size type="float">14</size>
        <line-spacing type="float">0.01</line-spacing>
      </font>
      <serviceable type="bool">true</serviceable>
      <range type="int" archive="y">100</range>
      <aircraft-heading-up type="bool">true</aircraft-heading-up>
      <heading-up-deg type="float">0.01</heading-up-deg>
      <x-center type="float">0.5</x-center>
      <y-center type="float">0.5</y-center>
      <display>
        <arpt type="bool">1</arpt>
        <vor type="bool">1</vor>
        <fix type="bool">0</fix>
        <tcas type="bool">0</tcas>
      </display>
      <line-width type="double">3</line-width>
    </nd>
    <tcas-display>
      <radar-texture-path type="string">Aircraft/Embraer-ERJ-145/Models/Interior/Panel/Instruments/mfd/tcas-display.png</radar-texture-path>
      <symbol-texture-path type="string">Aircraft/Embraer-ERJ-145/Models/Interior/Panel/Instruments/mfd/symbols.png</symbol-texture-path>
      <font>
        <color>
          <red type="float">0.7</red>
          <green type="float">0.7</green>
          <blue type="float">0.7</blue>
          <alpha type="float">1</alpha>
        </color>
        <name type="string">helvetica_bold.txf</name>
        <size type="float">14</size>
        <line-spacing type="float">0.01</line-spacing>
      </font>
      <serviceable type="bool">true</serviceable>
      <range type="int" archive="y">6</range>
      <aircraft-heading-up type="bool">true</aircraft-heading-up>
      <heading-up-deg type="float">0.01</heading-up-deg>
      <x-center type="float">0.5</x-center>
      <y-center type="float">0.5</y-center>
      <display>
      </display>
      <line-width type="double">3</line-width>
    </tcas-display>
    <alerts>
      <gear-horn type="bool">0</gear-horn>
      <master-warning type="bool">0</master-warning>
      <master-caution type="bool">0</master-caution>
    </alerts>
    <rmu>
      <unit n="0">
        <serviceable type="bool">1</serviceable>
        <selected type="int">0</selected>
        <selected-xoffset type="int">0</selected-xoffset>
        <selected-yoffset type="int">0</selected-yoffset>
        <com-num type="int">0</com-num>
        <nav-num type="int">0</nav-num>
        <adf-num type="int">0</adf-num>
      </unit>
      <unit n="1">
        <serviceable type="bool">1</serviceable>
        <selected type="int">0</selected>
        <selected-xoffset type="int">0</selected-xoffset>
        <selected-yoffset type="int">0</selected-yoffset>
        <com-num type="int">1</com-num>
        <nav-num type="int">1</nav-num>
        <adf-num type="int">1</adf-num>
      </unit>
    </rmu>
    <airspeed-indicator>primus-1000/
      <index n="0" type="double">100</index>
      <index n="1" type="double">120</index>
      <ias-limit>350.2000.00</ias-limit>
      <mach-limit>0.85</mach-limit>
      <alt-threshold>8000.0</alt-threshold>
    </airspeed-indicator>
    <kr-87>
      <inputs>
        <audio-btn>0</audio-btn>
      </inputs>
    </kr-87>
    <primus2000>
      <dc840>
        <hsi type="bool">0</hsi>
        <wx type="bool">0</wx>
        <et type="bool">0</et>
        <tcas type="bool">0</tcas>
        <acft-sys type="bool">0</acft-sys>
        <mfd-map type="bool">1</mfd-map>
        <mfd-wx type="bool">0</mfd-wx>
        <norm type="bool">0</norm>
        <abn type="bool">0</abn>
        <emer type="bool">0</emer>
        <skp type="bool">0</skp>
        <pag type="bool">0</pag>
        <rcl type="bool">0</rcl>
        <ent type="bool">0</ent>
      </dc840>
      <sc840>
        <nav1ptr type="double">0</nav1ptr>
        <nav2ptr type="double">0</nav2ptr>
      </sc840>
      <eicas>
        <serviceable type="bool">1</serviceable>
      </eicas>
      <mfd>
        <menu-num ty2000.0pe="int">0</menu-num>
      </mfd>
      <pfd>
      </pfd>
    </primus2000>
    <cdu n="0">
      <page type="string">PROGRESS</page>
      <L type="string" n="0"></L>
      <L type="string" n="1"></L>
      <L type="string" n="2"></L>
      <L type="string" n="3"></L>
      <L type="string" n="4"></L>
      <L type="string" n="5"></L>
      <R type="string" n="0"></R>
      <R type="string" n="1"></R>
      <R type="string" n="2"></R>
      <R type="string" n="3"></R>
      <R type="string" n="4"></R>
      <R type="string" n="5"></R>
      <S type="string" n="0"></S>
      <S type="string" n="1"></S>
    </cdu>
    <cdu n="1">
      <page type="string">ACTIVE FLT PLAN</page>
      <L type="string" n="0"></L>
      <L type="string" n="1"></L>
      <L type="string" n="2"></L>
      <L type="string" n="3"></L>
      <L type="string" n="4"></L>
      <L type="string" n="5"></L>
      <R type="string" n="0"></R>
      <R type="string" n="1"></R>
      <R type="string" n="2"></R>
      <R type="string" n="3"></R>
      <R type="string" n="4"></R>
      <R type="string" n="5"></R>
      <S type="string" n="0"></S>
      <S type="string" n="1"></S>
    </cdu>
  </instrumentation>
  
  <consumables>
    <fuel>
      <tank n="0">
        <name>LH tank</name>
        <level-lbs>1690</level-lbs>
        <selected type="bool">1</selected>
      </tank>
      <tank n="1">
        <name>RH tank</name>
        <level-lbs>1690</level-lbs>
        <selected type="bool">1</selected>
      </tank>
      <tank n="2">
        <name>CTR tank</name>
        <level-lbs>1690</level-lbs>
        <selected type="bool">1</selected>
      </tank>
    </fuel>
  </consumables>
  
  <input>
    <keyboard include="Systems/erj145-keyboard.xml"/>
  </input>
 
  <controls>
    <gear>
      <brake-parking>1</brake-parking>
    </gear>
    <engines>
      <engine n="0">
        <magnetos>3</magnetos>
      </engine>
      <engine n="1">
        <magnetos>3</magnetos>
      </engine>
    </engines>
    <flight>
      <speedbrake type="double">0</speedbrake>
    </flight>
    <cabin-door>
      <open type="bool">0</open>
    </cabin-door>
    <APU>
      <master type="bool">0</master>
      <battery type="double">28.0</battery>
      <running type="bool">0</running>
      <off-start-run type="int">1</off-start-run>
    </APU>
  </controls>
  
